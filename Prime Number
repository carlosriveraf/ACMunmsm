#include <bits/stdc++.h>
using namespace std;

bool primo(int x){
	
	if(x==2){return true;}
	else if(!(x&1)){return false;}
	int i=3;
	for(;i*i<=x;i+=2){
		if(x%i==0){return false;}
	}
	return true;
}

int main(){
	//Escribe el cÃ³digo
	//Output 1 si es primo
	//Output 0 si no es primo
	return 0;
}
